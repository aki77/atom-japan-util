{
  "name": "japan-util",
  "main": "./lib/main",
  "version": "0.3.0",
  "description": "utilities for Japanese",
  "keywords": [
    "japanese",
    "zenkaku",
    "hankaku",
    "hiragana",
    "katakana"
  ],
  "activationCommands": {
    "atom-text-editor": [
      "japan-util:hiragana",
      "japan-util:katakana",
      "japan-util:katakana-hankaku",
      "japan-util:hankaku",
      "japan-util:hankaku-only-ascii",
      "japan-util:hankaku-only-katakana",
      "japan-util:zenkaku",
      "japan-util:zenkaku-only-katakana"
    ]
  },
  "repository": "https://github.com/aki77/atom-japan-util",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "moji": "^0.5.1"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.7.0"
  }
}
